    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@babel/preset-typescript": "^7.8.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@datorama/akita": "^5.2.5",
    "@redactie/module-webpack-plugin": "2.0.0",
    "@redactie/react-router-guards": "1.0.2",
    "@redactie/redactie-core": "^1.10.5",
    "@redactie/schema-to-yup": "1.9.25",
    "@redactie/translations-module": "^0.8.4",
    "@redactie/utils": "^1.3.5",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.4.0",
    "@types/classnames": "2.2.9",
    "@types/jest": "24.9.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/ramda": "0.27.3",
