{
  "name": "@redactie/form-renderer-module",
  "version": "1.8.12-0",
  "description": "A dynamic form component for react using a JSON schema",
  "main": "dist/redactie-form-renderer-module.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "webpack --env.prod && npm run build:types",
    "build:w": "webpack -w --env.prod && npm run build:types",
    "build:demo": "webpack -w --env.local && npm run build:types",
    "build:analyse": "webpack --env.analyse && npm run build:types",
    "start": "npx concurrently --kill-others \"npm run build:demo\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .js,.jsx,.ts,.tsx ./public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public"
  },
  "keywords": [
    "Redactie",
    "Form",
    "React",
    "Module"
  ],
  "publishConfig": {
    "@redactie:registry": "https://nexusrepo.antwerpen.be/repository/npm-private"
  },
  "author": "glenn.verschooren@studiohyperdrive.be",
  "license": "ISC",
  "devDependencies": {
    "@acpaas-ui/react-components": "5.0.2",
